<Event>
<Name>On Post Install</Name>
<Args/>
<Script>-- Find / replace %AppFolder% in the template .workbench config.xml
function replaceAppFolder(variable, fileName)
	local template = TextFile.ReadToString(fileName);
	local result = String.Replace(template, variable, SessionVar.Expand(variable))
	TextFile.WriteFromString(fileName, result);
end

replaceAppFolder("%AppFolder%", SessionVar.Expand("%AppFolder%") .. "\\workbench.template\\.workbench\\config.xml");
replaceAppFolder("%AppFolder%", SessionVar.Expand("%AppFolder%") .. "\\towerd\\plugins-release.cfg");
replaceAppFolder("%AppFolder%", SessionVar.Expand("%AppFolder%") .. "\\towerd\\plugins-debug.cfg");
replaceAppFolder("%AppFolder%", SessionVar.Expand("%AppFolder%") .. "\\towerd\\client.xml");

INIFile.SetValue(SessionVar.Expand("%AppFolder%") .. "\\zpkg-config.ini", "General", "DeveloperRole", SessionVar.Expand("%DeveloperRole%"));
</Script>
<BookMarks count="0">
</BookMarks>
</Event>
<Event>
<Name>On Shutdown</Name>
<Args/>
<Script>result = Shell.Execute(SessionVar.Expand("%AppFolder%\\UpdateZOSS.exe"), "open", "", "", SW_SHOWNORMAL, true);

-- Call this function at the end after all other On Shutdown actions.
-- This will cause the system to be rebooted, if needed.
-- Defined in _SUF70_Global_Functions.lua
-- Do not edit this script unless you know what your are doing
g_HandleSystemReboot();
</Script>
<BookMarks count="0">
